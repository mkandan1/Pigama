{
    "version": 2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        },
        {
            "src":"./client/build",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/.*",
            "dest":"client/build"
        }
    ],
    "rewrites":  [
        {"source": "/(.*)", "destination": "index"}
      ]
}
